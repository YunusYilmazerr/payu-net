



@model Payu.WebSamples.Models.ReportResponseModel
@{
    ViewBag.Title = "ProductReport";
}

<h2>Sipariş Raporları</h2>
<form action="" method="post" class="form-horizontal">
    <fieldset>

        <!-- Form Name -->
        <!-- Text input-->
        <div class="form-group">
            <label class="col-md-4 control-label" for="">Başlangıç Tarihi</label>
            <div class="col-md-4">
                <input name="startDate" value="@DateTime.Now.ToString("yyyy-MM-dd")" class="form-control input-md">

            </div>
        </div>


        <div class="form-group">
            <label class="col-md-4 control-label" for=""> Bitiş Tarihi </label>
            <div class="col-md-4">
                <input value="@DateTime.Now.ToString("yyyy-MM-dd")" name="endDate" class="form-control input-md" width="50px">


            </div>
        </div>
    </fieldset>


    <!-- Button -->
    <div class="form-group">
        <label class="col-md-4 control-label" for=""></label>
        <div class="col-md-4">
            <button type="submit" id="" name="" class="btn btn-success">Sipariş Raporu Sorgula</button>
        </div>
    </div>


    <pre>
        @{
         if (Model!=null)
         {
                @Model.Response
         }
     
        }
</pre>
</form>